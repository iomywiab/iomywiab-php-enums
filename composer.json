{
  "name": "iomywiab/iomywiab-php-enums",
  "description": "Basic wrapper classes for enumerations",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "iomywiab",
      "email": "iomywiab@premium-postfach.de"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "psr/container": "^1.0",
    "iomywiab/iomywiab-php-constraints": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "iomywiab\\iomywiab_php_enums\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iomywiab\\iomywiab_php_constraints_tests\\": "vendor/iomywiab/iomywiab-php-constraints/tests",
      "iomywiab\\iomywiab_php_enums_tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/iomywiab/iomywiab-php-constraints.git"
    }
  ]
}
